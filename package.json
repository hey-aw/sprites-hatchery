{
  "name": "sprite-shell",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:proxy": "tsx watch --env-file=.env.local server/ws-proxy.ts",
    "dev:all": "concurrently \"pnpm dev\" \"pnpm dev:proxy\"",
    "build": "next build",
    "start": "next start",
    "start:proxy": "tsx --env-file=.env.local server/ws-proxy.ts",
    "start:all": "concurrently \"pnpm start\" \"pnpm start:proxy\"",
    "lint": "eslint"
  },
  "dependencies": {
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/addon-web-links": "^0.12.0",
    "@xterm/xterm": "^6.0.0",
    "jose": "^6.1.3",
    "next": "16.1.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.18.1",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "16.1.3",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
